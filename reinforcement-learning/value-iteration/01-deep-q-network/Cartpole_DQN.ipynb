    "agent = DQNAgent()\n",
    "scores, episodes = [], []\n",
    "\n",
    "for e in range(1, EPISODES+1):\n",
    "    state = env.reset()\n",
    "    steps = 0\n",
    "    while True:\n",
    "        #env.render()\n",
    "        state = torch.FloatTensor([state])\n",
    "        action = agent.act(state)\n",
    "        next_state, reward, done, _ = env.step(action.item())\n",
    "\n",
    "        # negative reward when attempt ends\n",
    "        if done:\n",
    "            reward = -100\n",
    "\n",
    "        agent.memorize(state, action, reward, next_state)\n",
    "        agent.learn()\n",
    "\n",
    "        state = next_state\n",
    "        steps += 1\n",
    "\n",
    "        if done:\n",
    "            print('Episode: ', e, ' Score: ', steps)\n",
    "            scores.append(steps)\n",
    "            episodes.append(e)\n",
    "            pylab.plot(episodes, scores, 'b')\n",
    "            if e % 1 == 0:            \n",
    "                agent.update_target_net()\n",
    "    \n",
    "        if done:\n",
    "            #env.close()\n",
    "            break\n",
    "    \n",
    "    if np.mean(scores[-min(5, len(scores)):]) == 500:\n",
    "        #env.close()\n",
    "        break\n"
   ]
